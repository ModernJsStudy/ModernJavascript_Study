//24-1
const x = 1;
function outerFunc() {
    const x = 10;

    function innerFunc() {
        console.log(x); //10
    }
        innerFunc(); 
}

outerFunc();


//24-2
const x = 1;
function outerFunc() {
    const x = 10;
    innerFunc();
}

function innerFunc() {
    console.log(x); //1
}

outerFunc();